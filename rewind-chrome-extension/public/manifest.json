{
  "name": "Solid Rewind",
  "description": "SolidJS timetravel debugger",
  "version": "1.1",
  "manifest_version": 3,
  "devtools_page": "devtools.html",
  "background": {
    "service_worker" : "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["context-script.js"]
    }
  ],
  "permissions": [],
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  }
}